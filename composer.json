{
  "name": "puzzle9/laravel-user-blockable",
  "description": "Laravel user block system",
  "keywords": [
    "laravel",
    "user",
    "block"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "hareku",
      "email": "hareku908@gmail.com"
    },
    {
      "name": "barisdemirhan",
      "email": "barisdemirhn@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/database": ">=5.4",
    "illuminate/support": ">=5.4",
    "nesbot/carbon": ">=2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Hareku\\LaravelBlockable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hareku\\LaravelBlockable\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hareku\\LaravelBlockable\\BlockableServiceProvider"
      ]
    }
  }
}
